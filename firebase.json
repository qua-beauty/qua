{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/api/bot",
        "function": "bot"
      },
      {
        "source": "/api/createCustomToken",
        "function": "createCustomToken"
      },
      {
        "source": "/api/answerWebAppQuery",
        "function": "answerWebAppQuery"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "public, max-age=600, s-maxage=1200"
      } ]
    }, {
      "source": "**/*.@(jpg|jpeg|gif|png)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "public, max-age=600, s-maxage=1200"
      } ]
    }]
  },
  "functions": {
    "source": "functions"
  }
}
